	wait 1
	Browser("DPLK").Page("Entry Penerimaan Teller Perorangan").WebElement("Data Nomor Rekening").Click
	wait 3
	Browser("DPLK").Page("Entry Penerimaan Teller Perorangan").WebElement("Dropdown Kantor").Click
	wait 1
	objkey.SendKeys(kantor)
	wait 1
	objkey.SendKeys"{ENTER}"
	wait 1
	Browser("DPLK").Page("Entry Penerimaan Teller Perorangan").WebEdit("Field Tanggal Transaksi").Set tglTransaksi
	wait 1
	Browser("DPLK").Page("Entry Penerimaan Teller Perorangan").WebEdit("Field Nominal Penerimaan").Set nominalPenerimaan
	wait 1
	Browser("DPLK").Page("Entry Penerimaan Teller Perorangan").WebEdit("Field Keterangan").Set keterangan
	
	If sumberDana <> "" Then
		wait 1
		Browser("DPLK").Page("Entry Penerimaan Teller Perorangan").WebEdit("Field Sumber Dana").Set sumberDana
	End If
	If tujuanTransaksi <> "" Then
		wait 1
		Browser("DPLK").Page("Entry Penerimaan Teller Perorangan").WebEdit("Field Tujuan Transaksi").Set tujuanTransaksi
	End If

	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Entry Penerimaan Teller Perorangan"), "Isi Fields yang Dibutuhkan, Kemudian Klik Button Simpan", " ", compatibilityMode.Desktop, ReportStatus.Done)
	wait 2
	Browser("DPLK").Page("Entry Penerimaan Teller Perorangan").WebButton("Btn Simpan").Click
	wait 2
		If Browser("DPLK").Page("Entry Penerimaan Teller Perorangan").WebElement("PopUp ×Data berhasil ditambah").Exist(5) Then
