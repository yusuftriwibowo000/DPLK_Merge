	Call CaptureImageUFTV2(Browser("DPLK").Page("Entry Penerimaan Teller Perorangan").WebButton("Btn Search No.Rekening Dplk"), "Klik Button Search No.Rekening DPLK", " ", compatibilityMode.Desktop, ReportStatus.Done)
