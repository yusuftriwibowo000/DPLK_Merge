Sub Bandingkan_No_Akun_Setup_Akuntansi_Setup_Jurnal_Standar
	Dim Text
	Jumlah_Row = Browser("DPLK").Page("Setup Jurnal Standar").WebTable("Tbl Journal Standar").RowCount
	No_Akun_Batch_Split = Split(No_Akun_Batch,",")
	
	For Data_Pada_Tabel = 2 To Jumlah_Row Step 1
		No_Akun = Browser("DPLK").Page("Setup Jurnal Standar").WebTable("Tbl Journal Standar").GetCellData(Data_Pada_Tabel,2)	
		For Cek_Data_Index = 0 To UBound(No_Akun_Batch_Split) Step 1
			If No_Akun = No_Akun_Batch_Split(Cek_Data_Index) Then
				Text = Text & " Nomer Akun " & No_Akun & " Sudah Terverifikasi,"
				Exit for
			End If
		Next
	Next
	
	If len(Text) > 1 Then
		Text = Left(Text,len(Text)-1)
	End If
	
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Jurnal Standar"),"Berikut Hasil Perbandingan Nomer Akun ",  Text ,  compatibilityMode.Desktop, ReportStatus.Passed)
	wait 2
	No_Akun_Batch = ""
End Sub

