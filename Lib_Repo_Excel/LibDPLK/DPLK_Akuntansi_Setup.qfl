		wait 2
		Call CaptureImageUFTV2(Browser("DPLK").Page("Verifikasi"),"Ubah Status Register Menjadi Lanjutkan Ke Verifikasi, Dan Tekan Simpan", " ",  compatibilityMode.Desktop, ReportStatus.Done)
