Sub Get_Var_To_Clipboard(var)
	Set objkey = CreateObject("WScript.Shell")
	
	objkey.SendKeys "{F6}"
	wait 2
	objkey.SendKeys "{BACKSPACE}"
	wait 1
	objkey.SendKeys Var
	wait 1
	objkey.SendKeys "^a"
	wait 1
	objkey.SendKeys "^c"
	wait 2
	objkey.SendKeys "{ESC}"
End Sub

Sub Split_Clipboard_To_Var(splitter)
