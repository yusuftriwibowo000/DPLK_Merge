	Browser("DPLK").Page("Setup Mapping Jurnal Inv").WebEdit("Field Search Data").Set noUrut
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Mapping Jurnal Inv"), "Search Data yang Akan Diupdate", " ", compatibilityMode.Desktop, ReportStatus.Done)
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Mapping Jurnal Inv").WebElement("Btn Edit"), "Klik Button Edit", " ", compatibilityMode.Desktop, ReportStatus.Done)
	wait 2
	Browser("DPLK").Page("Setup Mapping Jurnal Inv").WebElement("Btn Edit").Click
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Mapping Jurnal Inv"), "Berikut Detail Data Sebelum Diubah", " ", compatibilityMode.Desktop, ReportStatus.Done)
