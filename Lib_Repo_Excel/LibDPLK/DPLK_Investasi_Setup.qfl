'Approve Setup Portofolio
Sub ApproveSetupPortofolio
	Dim portoId
	portoId				= DataTable.Value("PORTO_ID", dtlocalsheet)
	statusVerifikasi	= DataTable.Value("STATUS_VERIFIKASI", dtlocalsheet)
	keteranganVerifikasi= DataTable.Value("KETERANGAN_VERIFIKASI", dtlocalsheet)
	explain				= DataTable.Value("EXPLAIN", dtlocalsheet)
	
	wait 2
	Browser("DPLK").Page("Setup Portofolio").WebEdit("Field Search Data").Set portoId
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Portofolio"), "Search Data yang Akan "&explain, " ", compatibilityMode.Desktop, ReportStatus.Done)
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Portofolio").WebElement("Btn Checklist Verifikasi"), "Klik Button Checklist", " ", compatibilityMode.Desktop, ReportStatus.Done)
	wait 2
	Browser("DPLK").Page("Setup Portofolio").WebElement("Btn Checklist Verifikasi").Click
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Portofolio"), "Berikut Detail Data Sebelum "&explain, " ", compatibilityMode.Desktop, ReportStatus.Done)
	wait 2
	objkey.SendKeys"{PGDN}"
	wait 1
	Browser("DPLK").Page("Setup Portofolio").WebElement("Dropdown Status Verifikasi").Click
	wait 1
	Browser("DPLK").Page("Setup Portofolio").WebEdit("Field Search Data").Click
	wait 1
	objkey.SendKeys(statusVerifikasi)
	wait 1
	objkey.SendKeys"{ENTER}"
	wait 1
	Browser("DPLK").Page("Setup Portofolio").WebEdit("Field Keterangan Verifikasi").Set keteranganVerifikasi
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Portofolio"), "Isi Field Status Verifikasi dan Keterangan Verifikasi, Kemudian Klik Button Simpan", " ", compatibilityMode.Desktop, ReportStatus.Done)
	wait 2
	Browser("DPLK").Page("Setup Portofolio").WebButton("Btn Simpan").Click
	wait 2
	Call ConfirmOtorisasi()
	If Browser("DPLK").Page("Setup Portofolio").WebElement("PopUp ×Data berhasil diupdate").Exist(5) Then
		Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Portofolio"), "Data Berhasil "&explain, " ", compatibilityMode.Desktop, ReportStatus.Passed)
		wait 2
		Browser("DPLK").Page("Setup Portofolio").WebEdit("Field Search Data").Set portoId
		wait 2
		Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Portofolio"), "Berikut Data Dengan Porto Id "&portoId&" Setelah "&explain, " ", compatibilityMode.Desktop, ReportStatus.Done)
		wait 2
		x = Browser("DPLK").Page("Setup Portofolio").WebElement("Tabel Data Setup Portofolio").GetROProperty("abs_x")
		y = Browser("DPLK").Page("Setup Portofolio").WebElement("Tabel Data Setup Portofolio").GetROProperty("abs_y")
		devices.MouseMove x,y
		devices.MouseClick x,y, LEFT_MOUSE_BUTTON
		wait 2
		objkey.SendKeys "{RIGHT 10}"
		wait 2
		Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Portofolio"), "Berikut Data Dengan Porto Id "&portoId&" Setelah "&explain, " ", compatibilityMode.Desktop, ReportStatus.Done)
	Else
		Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Portofolio"), "Data Gagal "&explain, " ", compatibilityMode.Desktop, ReportStatus.Passed)
	End If
	wait 2
End Sub

