	Browser("DPLK").Page("Setup Mapping Jurnal Inv").WebEdit("Field Search Data").Click
	wait 1
	objkey.SendKeys(portoID)
	wait 1
	objkey.SendKeys"{ENTER}"
	wait 1
	Browser("DPLK").Page("Setup Mapping Jurnal Inv").WebElement("Dropdown Kegiatan").Click
	wait 1
	Browser("DPLK").Page("Setup Mapping Jurnal Inv").WebEdit("Field Search Data").Click
