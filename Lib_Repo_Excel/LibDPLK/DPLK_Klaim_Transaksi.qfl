Sub Hitung_Calculate_Klaim_With_Status
	Set objkey = CreateObject("WScript.Shell")
	Call Uncheck_Calculate_Klaim_With_Status
	If DataTable.Value("CHECKLIST_STATUS",dtlocalsheet) <> "" Then
		Jenis_Status = split(DataTable.Value("CHECKLIST_STATUS",dtlocalsheet),",")
		For Iterator = 0 To UBound(Jenis_Status) Step 1
			If ucase(Jenis_Status(Iterator)) = "KHUSUS" Then
				Browser("DPLK").Page("Calculate Klaim").WebCheckBox("Check Box Status Khusus").Click
				Browser("DPLK").Page("Calculate Klaim").WebElement("Dropdown Status Khusus").Click
				wait 1
				objkey.SendKeys DataTable.Value("JENIS_STATUS_KHUSUS",dtlocalsheet)
				wait 2
				objkey.SendKeys "{ENTER}"
				wait 2
			ElseIf ucase(Jenis_Status(Iterator)) = "ANUITAS" Then
				Browser("DPLK").Page("Calculate Klaim").WebCheckBox("Check Box Dianuitaskan").Click
			ElseIf ucase(Jenis_Status(Iterator)) = "LUMPSUM" Then
				
			End If
		Next
	End If
	
	Call CaptureImageUFTV2(Browser("DPLK").Page("Calculate Klaim"),"Checklist Dan Isi Status Yang Diperlukan", " ",  compatibilityMode.Desktop, ReportStatus.Done)
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Calculate Klaim").WebButton("Btn Hitung"),"Tekan Button Hitung", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
	wait 2
	Browser("DPLK").Page("Calculate Klaim").WebButton("Btn Hitung").Click
	wait 300
	Call Validasi_Isi_Field_Calculate_Klaim
End Sub

Sub Uncheck_Calculate_Klaim_With_Status
	Status_Khusus = Browser("DPLK").Page("Calculate Klaim").WebCheckBox("Check Box Status Khusus").GetROProperty("checked")
	Status_Dianuitaskan = Browser("DPLK").Page("Calculate Klaim").WebCheckBox("Check Box Dianuitaskan").GetROProperty("checked")

	If Status_Khusus = 1 Then
		Browser("DPLK").Page("Calculate Klaim").WebCheckBox("Check Box Status Khusus").Click
	End If
	
	If Status_Dianuitaskan = 1 Then
		Browser("DPLK").Page("Calculate Klaim").WebCheckBox("Check Box Dianuitaskan").Click
	End If
End Sub

