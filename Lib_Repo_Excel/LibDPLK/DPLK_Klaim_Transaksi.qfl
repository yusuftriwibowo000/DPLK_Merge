Sub Tambah_Transaksi_Register_Isi_Dokumen_Klaim
	Set objkey = CreateObject("WScript.Shell")
	'membuat path DPLK file upload
	tempDPLKPath 	= Environment.Value("TestDir")
	tempDPLKPath2 	= InStrRev(tempDPLKPath, "\")
	pathDPLK		= Left(tempDPLKPath, tempDPLKPath2)
	uploadPathDPLK	= pathDPLK & "File Upload\"
	
	File = uploadPathDPLK & DataTable.Value("FILE_PDF",dtlocalsheet)
	Browser("DPLK").Page("Register Klaim").WebEdit("Field Id Peserta").Click
	wait 2
	For Iterator = 1 To DataTable.Value("DOKUMEN_UPLOAD",dtlocalsheet) Step 1
		wait 2
		objkey.SendKeys "{TAB}"
		wait 2	
		objkey.SendKeys "{ENTER}"
		wait 10
		objkey.SendKeys File
		wait 7
		objkey.SendKeys "{ENTER}"
		wait 5
	Next
	
	wait 2
		Call CaptureImageUFTV2(Browser("DPLK").Page("Register Klaim"),"Upload Dokumen - Dokumen Yang Diperlukan", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
	wait 2
End Sub

