Sub Search_Nilai_Transaksi_Pasca_Kerja_dari_Entry_Jurnal_Transaksi(Search_Value)
	set objkey = CreateObject("WScript.Shell")
	Set PasteData = CreateObject("Mercury.Clipboard")
	Set devices = CreateObject("Mercury.DeviceReplay")
	Browser("DPLK").Page("Approval Klaim Pasca Kerja").WebEdit("Field Search").Set DataTable.Value("SEARCH_DOKUMEN_ID",dtlocalsheet)
	Browser("DPLK").Page("Approval Klaim Pasca Kerja").WebButton("Btn Remove all items").Click
	wait 2
	objkey.SendKeys "{ESC}"
	wait 3
	Browser("DPLK").Page("Approval Klaim Pasca Kerja").WebElement("Btn Lihat").Click
	wait 3
	For Iterator = 1 To 9 Step 1
		objkey.SendKeys "^="
		wait 1
	Next
	
	wait 2
	objkey.SendKeys "^f"
	wait 1
	objkey.SendKeys Search_Value
	wait 1
	objkey.SendKeys "{ENTER}"
	wait 1
	devices.MouseDblClick 879 , 420 , LEFT_MOUSE_BUTTON
	wait 1
	set TekanButton = CreateObject("WScript.Shell")
	wait 2
	TekanButton.SendKeys "^c"
	wait 3
	TekanButton.SendKeys "^C"
	wait 5
	TekanButton.SendKeys "{F6}"
	wait 5
	TekanButton.SendKeys "^0"
	wait 3	
	TekanButton.SendKeys "{F12}"
	wait 2
	TekanButton.SendKeys "{ESC}"
	wait 1
	TekanButton.SendKeys "{ESC}"
	wait 1
	Browser("DPLK").Page("Approval Klaim Pasca Kerja").WebElement("Header").Click
	wait 1
	TekanButton.SendKeys "^f"
	wait 1
	TekanButton.SendKeys "^f"
	wait 1
	TekanButton.SendKeys Search_Value
	wait 1
	TekanButton.SendKeys "{ENTER}"
	wait 2
	Browser("DPLK").Page("Approval Klaim Pasca Kerja").WebElement("Total Penarikan").Click
	wait 2
	If Search_Value = PasteData.GetText Then
		wait 2
		Call CaptureImageUFTV2(Browser("DPLK").Page("Approval Klaim Pasca Kerja"),"Nilai "&Keterangan&" Sama", "Sama - Sama "&Search_Value,  compatibilityMode.Desktop, ReportStatus.Passed)
		wait 2
	Else 
		wait 2
		Call CaptureImageUFTV2(Browser("DPLK").Page("Approval Klaim Pasca Kerja"),"Nilai "&Keterangan&" Berbeda", "Data Yang Dicari : "&Search_Value&" Data Yang ditemukan : "&PasteData.GetText ,  compatibilityMode.Desktop, ReportStatus.Failed)
		wait 2
	End If
	
	PasteData.Clear
End Sub

