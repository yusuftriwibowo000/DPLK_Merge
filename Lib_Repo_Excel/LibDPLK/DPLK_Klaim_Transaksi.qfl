	No_Kol = Split(DataTable.Value("NO_REKENING",dtlocalsheet),",")
	Jenis_Klaim = Split(DataTable.Value("JENIS_KLAIM",dtlocalsheet),",")
	Nama_Bank = Split(DataTable.Value("NAMA_BANK",dtlocalsheet),",")
	Rekening_Bank = Split(DataTable.Value("REKENING_BANK",dtlocalsheet),",")
	Pemilik_Rekening = Split(DataTable.Value("PEMILIK_REKENING",dtlocalsheet),",")
	For Iterator = 0 To UBound(No_Kol) Step 1
		Browser("DPLK").Page("Register Klaim Kolektif").WebElement("First Object On Table Kolektif").SetTOProperty "innertext",No_Kol(Iterator)
		If Browser("DPLK").Page("Register Klaim Kolektif").WebElement("First Object On Table Kolektif").Exist(1) Then
			x = Browser("DPLK").Page("Register Klaim Kolektif").WebElement("First Object On Table Kolektif").getRoProperty("abs_x")
			y = Browser("DPLK").Page("Register Klaim Kolektif").WebElement("First Object On Table Kolektif").getRoProperty("abs_y")
			devices.MouseMove x , y
			devices.MouseClick x , y , LEFT_MOUSE_BUTTON
			wait 1
			objkey.SendKeys "+{TAB}"
			wait 1
			objkey.SendKeys " "
			wait 1
			objkey.SendKeys "{TAB}"
			wait 1
			objkey.SendKeys Jenis_Klaim(Iterator)
			wait 1
			objkey.SendKeys "{TAB}"
			wait 1
			objkey.SendKeys Nama_Bank(Iterator)
			wait 1
			objkey.SendKeys "{TAB}"
			wait 1
			objkey.SendKeys Rekening_Bank(Iterator)
			wait 1
			objkey.SendKeys "{TAB}"
			wait 1
			objkey.SendKeys Pemilik_Rekening(Iterator)
			wait 3
			wait 1
			objkey.SendKeys "+{TAB}"
			wait 1
			wait 1
			objkey.SendKeys "+{TAB}"
			wait 1
			wait 1
			objkey.SendKeys "+{TAB}"
			wait 1
			objkey.SendKeys "+{TAB}"
			wait 5
		End If
	Next
	Browser("DPLK").Page("Register Klaim Kolektif").WebEdit("Field Tanggal Pengajuan").Click
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Register Klaim Kolektif"),"Isi Dan Centang Data Yang Diperelukan", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
