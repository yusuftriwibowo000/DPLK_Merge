Sub Kirim_Kalkulasi_Validasi_Klaim_Pasca_Kerja_With_Status
	Set objkey = CreateObject("WScript.Shell")
	Browser("DPLK").Page("Calculate Klaim Pasca Kerja").WebButton("Btn Remove all items").Click
	wait 2
	objkey.SendKeys "{ESC}"
	wait 4
	Call CaptureImageUFTV2(Browser("DPLK").Page("Calculate Klaim Pasca Kerja"),"Berikut Hasil Pencarian dan Lanjut ke Proses Hitung", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
	wait 2
	Browser("DPLK").Page("Calculate Klaim Pasca Kerja").WebEdit("Field Search").Set DataTable.Value("ID_PESERTA",dtlocalsheet)
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Calculate Klaim Pasca Kerja"),"Berikut Data Yang Akan Dihitung", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
	wait 3
	Browser("DPLK").Page("Calculate Klaim Pasca Kerja").Link("Btn Proses").Click
	Call Uncheck_Calculate_Klaim_Pasca_Kerja_With_Status
	If DataTable.Value("CHECKLIST_STATUS",dtlocalsheet) <> "" Then
		Jenis_Status = split(DataTable.Value("CHECKLIST_STATUS",dtlocalsheet),",")
		For Iterator = 0 To UBound(Jenis_Status) Step 1
			If ucase(Jenis_Status(Iterator)) = "KHUSUS" Then
				Browser("DPLK").Page("Calculate Klaim Pasca Kerja").WebCheckBox("Check Box Status Khusus").Click
				Browser("DPLK").Page("Calculate Klaim Pasca Kerja").WebElement("Dropdown Status Khusus").Click
				wait 1
				objkey.SendKeys DataTable.Value("JENIS_STATUS_KHUSUS",dtlocalsheet)
				wait 2
				objkey.SendKeys "{ENTER}"
				wait 2
			ElseIf ucase(Jenis_Status(Iterator)) = "ANUITAS" Then
				Browser("DPLK").Page("Calculate Klaim Pasca Kerja").WebCheckBox("Check Box Dianuitaskan").Click
			ElseIf ucase(Jenis_Status(Iterator)) = "LUMPSUM" Then
				
			End If
		Next
	End If
	
	Call CaptureImageUFTV2(Browser("DPLK").Page("Calculate Klaim Pasca Kerja"),"Checklist Dan Isi Status Yang Diperlukan", " ",  compatibilityMode.Desktop, ReportStatus.Done)
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Calculate Klaim Pasca Kerja").WebButton("Btn Hitung"),"Tekan Button Hitung", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
	wait 2
	Browser("DPLK").Page("Calculate Klaim Pasca Kerja").WebButton("Btn Hitung").Click
	wait 300
	Call Validasi_Isi_Field_Calculate_Klaim
End Sub

Sub Uncheck_Calculate_Klaim_Pasca_Kerja_With_Status
	Status_Khusus = Browser("DPLK").Page("Calculate Klaim Pasca Kerja").WebCheckBox("Check Box Status Khusus").GetROProperty("checked")
	Status_Dianuitaskan = Browser("DPLK").Page("Calculate Klaim Pasca Kerja").WebCheckBox("Check Box Dianuitaskan").GetROProperty("checked")

	If Status_Khusus = 1 Then
		Browser("DPLK").Page("Calculate Klaim Pasca Kerja").WebCheckBox("Check Box Status Khusus").Click
	End If
	
	If Status_Dianuitaskan = 1 Then
		Browser("DPLK").Page("Calculate Klaim Pasca Kerja").WebCheckBox("Check Box Dianuitaskan").Click
	End If
End Sub

