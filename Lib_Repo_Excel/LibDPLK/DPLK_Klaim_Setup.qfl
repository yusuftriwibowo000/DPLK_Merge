Sub Ubah_Setup_Dokumen_Jenis_Klaim
	Set objkey = CreateObject("WScript.Shell")
	Browser("DPLK").Page("Setup Dokumen Jenis Klaim").WebEdit("Field Search").Set DataTable.Value("KODE_JENIS_PENSIUN",dtlocalsheet)
	wait 5
	Browser("DPLK").Page("Setup Dokumen Jenis Klaim").WebElement("Btn Ubah").Click
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Dokumen Jenis Klaim"),"Masuk Menu Ubah Data ", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
	wait 2
	
	'Cek Kode Dokumen Yang Dipilih
	Kode_Dokumen = Split(DataTable.Value("KODE_DOKUMEN",dtlocalsheet),",")
	Jumlah_Row = Browser("DPLK").Page("Setup Dokumen Jenis Klaim").WebTable("Table Dokumen Jenis Klaim").RowCount()
	For Iterator1 = 1 To UBound(Kode_Dokumen) + 1 Step 1
		For Iterator2 = 1 To Jumlah_Row - 1 Step 1
			Row_Kode_Dokumen = Browser("DPLK").Page("Setup Dokumen Jenis Klaim").WebTable("Table Dokumen Jenis Klaim").GetCellData(1 + iterator2 ,2)
			If Kode_Dokumen(iterator1 - 1) = Row_Kode_Dokumen Then
				set x = Browser("DPLK").Page("Setup Dokumen Jenis Klaim").WebTable("Table Dokumen Jenis Klaim").ChildItem(1 + iterator2,1,"WebElement",0)
				x.click
			End If
		Next
	Next
	
	'Cek Mandatory yang dipilih
	Kode_Dokumen = Split(DataTable.Value("KODE_MANDATORY_DOKUMEN",dtlocalsheet),",")
	Jumlah_Row = Browser("DPLK").Page("Setup Dokumen Jenis Klaim").WebTable("Table Dokumen Jenis Klaim").RowCount()
	For Iterator1 = 1 To UBound(Kode_Dokumen) + 1 Step 1
		For Iterator2 = 1 To Jumlah_Row - 1 Step 1
			Row_Kode_Dokumen = Browser("DPLK").Page("Setup Dokumen Jenis Klaim").WebTable("Table Dokumen Jenis Klaim").GetCellData(1 + iterator2 ,2)
			If Kode_Dokumen(iterator1 - 1) = Row_Kode_Dokumen Then
				set CekBox = Browser("DPLK").Page("Setup Dokumen Jenis Klaim").WebTable("Table Dokumen Jenis Klaim").ChildItem(1 + iterator2,4,"WebElement",0)
				CekBox.click
				
			End If
		Next
	Next
	
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Dokumen Jenis Klaim"),"Ubah Field Yang Diperlukan Dan Tekan Simpan ", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
	wait 2
	
	If Jumlah_Row > 12 Then
		Set devices = CreateObject("Mercury.DeviceReplay")
		x = Browser("DPLK").Page("Setup Dokumen Jenis Klaim").WebTable("Table Dokumen Jenis Klaim").GetROProperty("abs_x")
		y = Browser("DPLK").Page("Setup Dokumen Jenis Klaim").WebTable("Table Dokumen Jenis Klaim").GetROProperty("abs_y")
		devices.MouseMove x , y
		devices.MouseClick x , y , LEFT_MOUSE_BUTTON
		objkey.SendKeys "{DOWN 10}"
		
		wait 2
		Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Dokumen Jenis Klaim"),"Ubah Field Yang Diperlukan Dan Tekan Simpan ", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
		wait 2
	End If
	
	
	Browser("DPLK").Page("Setup Dokumen Jenis Klaim").WebButton("Btn Simpan").Click
	wait 7
		If Browser("DPLK").Page("Setup Dokumen Jenis Klaim").WebElement("Notifikasi").Exist(2) Then
		TextNotif = Browser("DPLK").Page("Setup Dokumen Jenis Klaim").WebElement("Notifikasi").GetROProperty("innertext")
		TextNotifBerhasil = Instr(ucase(TextNotif),"BERHASIL")
	End If
	
	
	If TextNotifBerhasil <> 0 Then
		wait 2
		Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Dokumen Jenis Klaim"),mid(TextNotif,2,len(TextNotif)-1), " ",  compatibilityMode.Desktop, ReportStatus.Passed)
		wait 2
		Browser("DPLK").Page("Setup Dokumen Jenis Klaim").WebButton("Btn X Notifikasi").Click
	End If
End Sub

