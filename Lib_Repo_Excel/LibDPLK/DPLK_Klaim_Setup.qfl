
REM ==== Setup Klaim Status
Sub Tambah_Setup_Klaim_Status_Proses
	Browser("DPLK").Page("Setup Klaim Status Proses").WebButton("Btn Tambah").Click
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Klaim Status Proses"),"Masuk Menu Tambah Data ", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
	wait 2
	Browser("DPLK").Page("Setup Klaim Status Proses").WebEdit("Field Kode Status Proses").Set DataTable.Value("KODE_STATUS_PROSES",dtlocalsheet)
	Browser("DPLK").Page("Setup Klaim Status Proses").WebEdit("Field Nama Status Proses").Set DataTable.Value("NAMA_STATUS_PROSES",dtlocalsheet)
	Browser("DPLK").Page("Setup Klaim Status Proses").WebEdit("Field Kode Mapping").Set DataTable.Value("KODE_MAPPING",dtlocalsheet)
	Browser("DPLK").Page("Setup Klaim Status Proses").WebEdit("Field Desc Mapping").Set DataTable.Value("DESC_MAPPING",dtlocalsheet)
	wait 2
	Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Klaim Status Proses"),"Isi Field Yang Diperlukan Dan Tekan Simpan ", " ",  compatibilityMode.Desktop, ReportStatus.Passed)
	wait 2
	Browser("DPLK").Page("Setup Klaim Status Proses").WebButton("Btn Simpan").Click
	wait 7
	
	If Browser("DPLK").Page("Setup Klaim Status Proses").WebElement("Notifikasi").Exist(2) Then
		TextNotif = Browser("DPLK").Page("Setup Klaim Status Proses").WebElement("Notifikasi").GetROProperty("innertext")
		TextNotifBerhasil = Instr(ucase(TextNotif),"BERHASIL")
	End If
	
	
	If TextNotifBerhasil <> 0 Then
		wait 2
		Call CaptureImageUFTV2(Browser("DPLK").Page("Setup Klaim Status Proses"),mid(TextNotif,2,len(TextNotif)-1), " ",  compatibilityMode.Desktop, ReportStatus.Passed)
		wait 2
		Browser("DPLK").Page("Setup Klaim Status Proses").WebButton("Btn X Notifikasi").Click
	End If
	
End Sub

